                <% let groupList=[]; if (typeof team !=='undefined' && team && team.length> 0) {
                    let unique = {};
                    for(let i=0; i<team.length; i++){ unique[team[i].group]=true; } let keys=Object.keys(unique); /*
                        Orden manual para los principales */ let manualOrder=[ 'Consejo de Seguridad'
                        , 'Asamblea General' , 'Sala de Tratados Internacionales' , 'Sala de Tratados' , 'Tratados'
                        , 'Consejo Econ√≥mico y Social' , 'ECOSOC' , 'Consejo de Derechos Humanos' , 'Derechos Humanos'
                        , 'CDH' , 'Log√≠stica' , 'Capacitaci√≥n' , 'Coordinaci√≥n General' ]; /* Identificar grupos de
                        Prensa para mover al final */ let prensaGroups=keys.filter(k=>
                        k.toLowerCase().includes('prensa') || k.toLowerCase().includes('comunicaci√≥n'));

                        /* Construir lista */
                        /* 1. Manuales (si existen) */
                        for(let i=0; i<manualOrder.length; i++){ if(unique[manualOrder[i]]) {
                            groupList.push(manualOrder[i]); unique[manualOrder[i]]=false; } } /* 2. Resto (excepto
                            Prensa) */ for(let i=0; i<keys.length; i++){ if(unique[keys[i]] &&
                            !prensaGroups.includes(keys[i])) { groupList.push(keys[i]); unique[keys[i]]=false; } } /* 3.
                            Prensa al final */ prensaGroups.forEach(pg=> {
                            if(unique[pg] !== false) groupList.push(pg);
                            });
                            }
                            %>

                            <% for(let g=0; g < groupList.length; g++) { let gName=groupList[g]; let members=[]; for(let
                                m=0; m<team.length; m++){ if(team[m].group===gName) members.push(team[m]); }
                                if(members.length> 0) {
                                let tColor = 'text-slate-800';
                                let cColor = 'gray';
                                let lowerName = gName.toLowerCase();
                                let displayName = gName;

                                /* Rename Sala de Tratados if needed */
                                if (gName === 'Sala de Tratados') displayName = 'Sala de Tratados Internacionales';

                                if(gName === 'Asamblea General' || lowerName.includes('asamblea')) {
                                cColor = 'green';
                                tColor = 'text-brand-green';
                                }
                                else if(gName === 'Consejo Econ√≥mico y Social' || lowerName.includes('econ√≥mico') ||
                                lowerName.includes('ecosoc')) {
                                cColor = 'orange'; /* ECOSOC -> Orange */
                                tColor = 'text-brand-orange';
                                }
                                else if(gName === 'Consejo de Seguridad' || lowerName.includes('seguridad')) {
                                cColor = 'red';
                                tColor = 'text-brand-red';
                                }
                                else if(gName === 'Sala de Tratados Internacionales' || gName === 'Sala de Tratados' ||
                                lowerName.includes('tratado')) {
                                cColor = 'yellow'; /* Sala Tratados -> Yellow */
                                tColor = 'text-brand-yellow';
                                }
                                else if(gName === 'Consejo de Derechos Humanos' || lowerName.includes('derechos') ||
                                lowerName.includes('cdh')) {
                                cColor = 'blue'; /* Derechos Humanos -> Celeste (Blue) */
                                tColor = 'text-brand-blue';
                                }
                                else if(lowerName.includes('prensa') || lowerName.includes('comunicaci√≥n')) {
                                cColor = 'pink';
                                tColor = 'text-pink-600';
                                }
                                else if(gName === 'Capacitaci√≥n' || lowerName.includes('capacitaci√≥n')) {
                                cColor = 'gray';
                                }
                                else if(gName === 'Coordinaci√≥n General' || lowerName.includes('proyectos')) {
                                cColor = 'slate';
                                }
                                %>
                                <div data-aos="fade-up">
                                    <div class="text-center mb-10">
                                        <h3 class="text-2xl font-black <%= tColor %>">
                                            <%= displayName %>
                                        </h3>
                                        <% if(gName==='Coordinaci√≥n General' ) { %>
                                            <div class="h-1 w-12 bg-slate-200 mx-auto mt-2 rounded-full"></div>
                                            <% } %>
                                    </div>
                                    <div class="flex flex-wrap justify-center gap-6 md:gap-8">
                                        <% for(let k=0; k<members.length; k++) { let mem=members[k]; let
                                            bColor='bg-gray-100 text-gray-500' ; if(cColor==='green' )
                                            bColor='bg-brand-green text-white' ; else if(cColor==='orange' )
                                            bColor='bg-brand-orange text-white' ; else if(cColor==='blue' )
                                            bColor='bg-brand-blue text-white' ; else if(cColor==='yellow' )
                                            bColor='bg-brand-yellow text-slate-900' ; else if(cColor==='red' )
                                            bColor='bg-brand-red text-white' ; else if(cColor==='pink' )
                                            bColor='bg-pink-600 text-white' ; else if(cColor==='slate' )
                                            bColor='bg-slate-800 text-white' ; %>
                                            <div class="flex flex-col items-center text-center group w-40 md:w-44">
                                                <div
                                                    class="w-32 h-32 md:w-36 md:h-36 rounded-full overflow-hidden shadow-sm border border-gray-100 mb-3 relative">
                                                    <img src="/img/Voluntarios/<%= mem.image %>"
