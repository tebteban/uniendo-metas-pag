                        </div>
                    </div>
                </div>
            </section>

            <!-- Swiper CSS -->
            <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css" />

            <section class="py-20 bg-white overflow-hidden border-t border-gray-100">
                <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
                    <div class="text-center mb-10">
                        <h3 class="text-3xl font-black text-slate-900">Galer√≠a de Voluntariado</h3>
                        <p class="text-gray-500 mt-2">Momentos inolvidables de nuestro equipo</p>
                    </div>

                    <% const volSliderKeys=Object.keys(settings).filter(function(k) { return k.startsWith('vol_slider_')
                        && settings[k]; }).sort(); const
                        volFallback=[ 'https://images.unsplash.com/photo-1529156069898-49953e39b3ac?auto=format&fit=crop&q=80'
                        , 'https://images.unsplash.com/photo-1511632765486-a01980e01a18?auto=format&fit=crop&q=80'
                        , 'https://images.unsplash.com/photo-1577962917302-cd874c4e3169?auto=format&fit=crop&q=80' ];
                        const volImages=volSliderKeys.length> 0 ? volSliderKeys.map(function(k) { return settings[k]; })
                        : volFallback;
                        %>

                        <!-- Swiper -->
                        <div class="swiper mySwiper w-full px-4 py-8">
                            <div class="swiper-wrapper pb-10">
                                <% for(const img of volImages) { %>
                                    <div class="swiper-slide h-auto">
                                        <div
                                            class="aspect-video w-full rounded-2xl overflow-hidden shadow-lg border border-gray-100 group relative">
                                            <img src="<%= img %>"
                                                class="w-full h-full object-cover transform group-hover:scale-110 transition duration-700"
                                                alt="Foto Voluntariado" loading="lazy">
                                            <div class="swiper-lazy-preloader swiper-lazy-preloader-white"></div>
                                            <div
                                                class="absolute inset-0 bg-black/0 group-hover:bg-black/10 transition duration-300">
                                            </div>
                                        </div>
                                    </div>
                                    <% } %>
                            </div>
                            <div class="swiper-pagination"></div>
                            <div
                                class="swiper-button-next !text-slate-800 after:!text-xl bg-white/80 p-6 rounded-full shadow-lg hover:bg-white transition hidden md:flex">
                            </div>
                            <div
                                class="swiper-button-prev !text-slate-800 after:!text-xl bg-white/80 p-6 rounded-full shadow-lg hover:bg-white transition hidden md:flex">
                            </div>
                        </div>
                </div>
            </section>

            <!-- Swiper JS -->
            <script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>
            <script>
                var swiper = new Swiper(".mySwiper", {
                    slidesPerView: 1,
                    spaceBetween: 24,
                    centeredSlides: false,
                    loop: true,
                    grabCursor: true,
                    autoplay: {
                        delay: 3000,
                        disableOnInteraction: false,
                        pauseOnMouseEnter: true
                    },
                    pagination: {
                        el: ".swiper-pagination",
                        clickable: true,
                        dynamicBullets: true,
                    },
                    navigation: {
                        nextEl: ".swiper-button-next",
                        prevEl: ".swiper-button-prev",
                    },
                    breakpoints: {
                        640: {
                            slidesPerView: 2,
                            spaceBetween: 24,
                        },
                        1024: {
                            slidesPerView: 3,
                            spaceBetween: 32,
                        },
                    },
                });


            </script>

            <%- include('./partials/footer') %>

                <div id="lightbox"
                    class="fixed inset-0 z-[200] bg-black/90 hidden flex items-center justify-center p-4 backdrop-blur-sm"
                    onclick="closeLightbox()">
                    <img id="lightbox-img" src="" class="max-w-full max-h-[90vh] rounded-lg shadow-2xl"
                        alt="Full screen">
                </div>

                <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
                <script>
                    AOS.init({ once: true, offset: 50, duration: 800 });
                    const lightbox = document.getElementById('lightbox');
                    const lightboxImg = document.getElementById('lightbox-img');


                    function openGalleryDirect(src) {
                        lightboxImg.src = src;
                        lightbox.classList.remove('hidden');
                        document.body.style.overflow = 'hidden';
                    }
                    function closeGallery() {
                        lightbox.classList.add('hidden');
                        document.body.style.overflow = 'auto';
                    }
                    window.closeLightbox = closeGallery;
                </script>
    </body>

</html>