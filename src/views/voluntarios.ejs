                                let tColor = 'text-slate-800';
                                let cColor = 'gray';
                                let lowerName = gName.toLowerCase();

                                if(gName === 'Asamblea General' || lowerName.includes('asamblea')) { cColor = 'green';
                                tColor = 'text-brand-green'; }
                                else if(gName === 'Consejo Econ√≥mico y Social' || lowerName.includes('econ√≥mico') ||
                                lowerName.includes('ecosoc')) { cColor = 'blue'; tColor = 'text-brand-blue'; }
                                else if(gName === 'Consejo de Seguridad' || lowerName.includes('seguridad')) { cColor =
                                'red'; tColor = 'text-brand-red'; }
                                else if(gName === 'Sala de Tratados Internacionales' || lowerName.includes('tratado')) {
                                cColor = 'orange'; tColor = 'text-brand-orange'; }
                                else if(gName === 'Consejo de Derechos Humanos' || lowerName.includes('derechos') ||
                                lowerName.includes('cdh')) { cColor = 'yellow'; tColor = 'text-brand-yellow'; }
                                else if(lowerName.includes('prensa') || lowerName.includes('comunicaci√≥n')) { cColor =
                                'pink'; tColor = 'text-pink-600'; }
                                else if(gName === 'Capacitaci√≥n' || lowerName.includes('capacitaci√≥n')) { cColor =
                                'gray'; }
                                else if(gName === 'Coordinaci√≥n General' || lowerName.includes('proyectos')) { cColor =
                                'slate'; }
                                %>
                                <div data-aos="fade-up">
                                    <div class="text-center mb-10">
                                        <h3 class="text-2xl font-black <%= tColor %>">
                                            <%= gName %>
                                        </h3>
                                        <% if(gName==='Coordinaci√≥n General' ) { %>
