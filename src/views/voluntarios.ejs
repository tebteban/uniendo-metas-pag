                <% let groupList=[]; if (typeof team !=='undefined' && team && team.length> 0) {
                    let unique = {};
                    for(let i=0; i<team.length; i++){ unique[team[i].group]=true; } let keys=Object.keys(unique); //
                        Orden manual para los principales let manualOrder=[ 'Consejo de Seguridad' , 'Asamblea General'
                        , 'Sala de Tratados Internacionales' , 'Sala de Tratados' , 'Tratados'
                        , 'Consejo Econ√≥mico y Social' , 'ECOSOC' , 'Consejo de Derechos Humanos' , 'Derechos Humanos'
                        , 'CDH' , 'Log√≠stica' , 'Capacitaci√≥n' , 'Coordinaci√≥n General' ]; // Identificar grupos de
                        Prensa para mover al final let prensaGroups=keys.filter(k=> k.toLowerCase().includes('prensa')
                        || k.toLowerCase().includes('comunicaci√≥n'));

                        // Construir lista
                        // 1. Manuales (si existen)
                        for(let i=0; i<manualOrder.length; i++){ if(unique[manualOrder[i]]) {
                            groupList.push(manualOrder[i]); unique[manualOrder[i]]=false; } } // 2. Resto (excepto
                            Prensa) for(let i=0; i<keys.length; i++){ if(unique[keys[i]] &&
                            !prensaGroups.includes(keys[i])) { groupList.push(keys[i]); unique[keys[i]]=false; } } // 3.
                            Prensa al final prensaGroups.forEach(pg=> {
                            if(unique[pg] !== false) groupList.push(pg);
                            });
                            }
                            %>

                            <% for(let g=0; g < groupList.length; g++) { let gName=groupList[g]; let members=[]; for(let
