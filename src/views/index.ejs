    <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
    <script>
        AOS.init({ once: true, offset: 50, duration: 800 });

        // LÓGICA DEL MENÚ MÓVIL (CORREGIDA)
        document.addEventListener('DOMContentLoaded', () => {
            const btn = document.getElementById('mobile-menu-button');
            const menu = document.getElementById('mobile-menu');
            if(btn && menu) {
                btn.addEventListener('click', () => menu.classList.toggle('hidden'));
                document.querySelectorAll('.mobile-link').forEach(link => {
                    link.addEventListener('click', () => menu.classList.add('hidden'));
                });
            }
        });

        // LÓGICA DEL MODAL (CORREGIDA)
        function openModal(modalId) {
            const modal = document.getElementById(modalId);
            const backdrop = document.getElementById('modal-backdrop');
            const panel = document.getElementById('modal-panel');
            if (modal) {
                modal.classList.remove('hidden');
                document.body.style.overflow = 'hidden';
                setTimeout(() => {
                    backdrop.classList.replace('opacity-0', 'opacity-100');
                    panel.classList.remove('opacity-0', 'scale-90');
                }, 10);
            }
        }

        function closeModal(modalId) {
            const modal = document.getElementById(modalId);
            const backdrop = document.getElementById('modal-backdrop');
            const panel = document.getElementById('modal-panel');
            if (modal) {
                backdrop.classList.replace('opacity-100', 'opacity-0');
                panel.classList.add('opacity-0', 'scale-90');
                setTimeout(() => {
                    modal.classList.add('hidden');
                    document.body.style.overflow = 'auto';
                }, 300);
            }
        }

        // LÓGICA DE CONTADORES
        const counters = document.querySelectorAll('.counter-animate');
        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    counters.forEach(counter => {
                        const target = +counter.getAttribute('data-target');
                        let count = 0;
                        const update = () => {
                            const inc = target / 50;
                            if (count < target) {
                                count += inc;
                                counter.innerText = Math.ceil(count);
                                setTimeout(update, 20);
                            } else counter.innerText = target;
                        };
                        update();
                    });
                    observer.disconnect();
                }
            });
        });
        if(document.getElementById('impacto')) observer.observe(document.getElementById('impacto'));
    </script>
    <script>
    document.addEventListener('scroll', function() {
        // Detectamos si es una computadora (ancho mayor a 768px)
        if (window.innerWidth > 768) {
            const imagen = document.getElementById('fondo-movil');
            const seccion = document.getElementById('impacto');
            
            if (imagen && seccion) {
                const rect = seccion.getBoundingClientRect();
                
                // Solo animamos si la sección es visible
                if (rect.top < window.innerHeight && rect.bottom > 0) {
                    const velocidad = 0.05;
                    // El -50 es para compensar el margen negativo de la sección
                    const movimientoX = (window.scrollY * velocidad) - 50;
                    
                    imagen.style.transform = `scale(1.1) translate3d(${movimientoX}px, 0, 0)`;
                }
            }
        } else {
            // Si es móvil, nos aseguramos que la imagen esté centrada y sin transformaciones locas
            const imagen = document.getElementById('fondo-movil');
            if (imagen) {
                imagen.style.transform = 'scale(1.2) translate3d(0, 0, 0)';
            }
        }
    });
    function toggleFaq(id) {
        const content = document.getElementById(id);
        const icon = document.getElementById('icon-' + id);
        
        if (content) {
            content.classList.toggle('hidden');
            if(icon) icon.classList.toggle('rotate-180');
        }
    }
</script>
